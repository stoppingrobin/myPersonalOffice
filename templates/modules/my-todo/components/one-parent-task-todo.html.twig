<div id="modal-wrapper-button-{{ todo.id }} " class="modal-wrapper-button trash-parent my-todo-one-parent-task-todo ">
    <button class="trigger-custom btn cur-p btn-outline-primary todo-title" data-toggle="modal"
            data-target="#todo{{ todo.id }}">
        {{ todo.name }}

        {% if
            (
                    todo.module is not null
                and MODULE_NAME_ISSUES == todo.module.name
                and todo.myIssue is null
            )
        %}
            <span class="my-todo-one-parent-task-todo-asterix">*</span>
        {% endif %}
    </button>
    <section class="col-md-6 todo-buttons d-flex justify-content-around">

    </section>
</div>