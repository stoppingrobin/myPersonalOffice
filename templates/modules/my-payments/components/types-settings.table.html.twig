<table id="MyPaymentsSettings" class="table table-striped table-bordered" cellspacing="0" data-table="true" width="100%">
    <thead>
    <tr>
        <td class="d-none">{{ 'datatables.table.id' | trans }}</td>
        <td>{{ 'payments.settings.types.table.name' | trans }}</td>
        <td>{{ 'datatables.table.actions' | trans }}</td>
    </tr>
    </thead>
    <tbody>
    {% for payment_type in payments_types %}
        <tr class="trash-parent car-single-data editable-parent save-parent" data-type="MyPaymentsSettings">
            <td class="d-none id">{{ payment_type.id }}</td>
            <td class="value"> {{ payment_type.value }}</td>

            {% include 'modules/common/components/table-cells/actions.html.twig' with {
                "trash": true,
                "edit": true,
                "save": true
            } %}

        </tr>
    {% endfor %}
    </tbody>
</table>
